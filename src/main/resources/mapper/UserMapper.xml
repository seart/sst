<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.latestspringsecurity.dao.UserMapper">


    <select id="findByUserName" resultType="org.example.latestspringsecurity.model.User">
        SELECT * FROM users WHERE username=#{username}
    </select>
</mapper>
